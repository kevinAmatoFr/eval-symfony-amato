<form method="post" action="{{ path('compagny_delete', {'id': compagny.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer cette entreprise?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ compagny.id) }}">
    <button  type="button" class="btn btn-danger">Supprimer</button>
</form>
